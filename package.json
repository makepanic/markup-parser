{
  "name": "markup-parser",
  "version": "0.2.1",
  "description": "A set of primitives for building string markup parsers",
  "scripts": {
    "pretest": "yarn build:test",
    "test": "nyc --reporter=html ava 'tests/**/*.js'",
    "posttest": "yarn bench",
    "bench": "node targets/bench",
    "build": "tsc",
    "build:es2015": "tsc -p tsconfig.es2015.json",
    "build:test": "tsc -p tsconfig.test.json",
    "build:gh-pages": "tsc -p tsconfig.gh-pages.json",
    "docs": "typedoc --out docs --mode file lib",
    "prettier": "prettier --write index.ts '{lib,markups,tests}/**/*.ts'",
    "prepublishOnly": "yarn test && yarn build && yarn types",
    "types": "dts-generator --name markup-parser --project . --out index.d.ts"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/makepanic/markup-parser.git"
  },
  "author": "Christian",
  "license": "MIT",
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/lodash": "^4.14.116",
    "ava": "^0.25.0",
    "benchmark": "^2.1.4",
    "dts-generator": "^2.1.0",
    "nyc": "^13.0.1",
    "open-color": "^1.5.1",
    "prettier": "^1.14.3",
    "spectre.css": "^0.2.14",
    "surku": "^0.1.14",
    "typedoc": "^0.12.0",
    "typescript": "^3.1.1"
  },
  "prettier": {
    "parser": "typescript"
  }
}
